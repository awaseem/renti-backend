{
  "name": "RESTful-express-starter",
  "version": "1.0.0",
  "description": "A simple RESTful API starter with Express and Mocha",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/awaseem/RESTful-express-starter.git"
  },
  "scripts": {
    "eslint": "eslint -c .eslintrc api config middlewares migrations models seeds test",
    "test": "mocha --compilers js:babel-register --timeout 15000",
    "dev": "nodemon app.js --exec babel-node --presets es2015",
    "start": "node main.js",
    "rebuild": "rm dev.sqlite3 && knex migrate:latest && knex seed:run",
    "postinstall": "knex migrate:latest && knex seed:run"
  },
  "author": "Ali Waseem",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.13.3",
    "bookshelf": "^0.9.1",
    "checkit": "^0.6.0",
    "express": "^4.13.3",
    "jsonwebtoken": "^5.7.0",
    "knex": "^0.9.0",
    "morgan": "^1.6.1",
    "nconf": "^0.8.2",
    "sqlite3": "^3.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "eslint": "^2.5.1",
    "eslint-plugin-react": "^3.15.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "supertest": "^1.1.0"
  }
}
